<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">
    <androidx.appcompat.widget.LinearLayoutCompat android:layout_width="wrap_content"
                                                  android:layout_height="wrap_content"
                                                  android:orientation="vertical" android:id="@+id/linearLayoutCompat2"
                                                  app:layout_constraintTop_toTopOf="parent"
                                                  app:layout_constraintStart_toStartOf="parent">
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                  android:text="Set current timer to 15 min" android:id="@+id/setTimerTimer"
                  app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
        />
        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Clear all committed times for today"
                android:id="@+id/clearCommitedTimesButton"
        />
    </androidx.appcompat.widget.LinearLayoutCompat>
</androidx.constraintlayout.widget.ConstraintLayout>